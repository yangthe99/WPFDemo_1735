<?xml version="1.0" encoding="utf-8" ?> <!--文件宣告-->
<configuration> <!--配置設定-->
	<connectionStrings> <!--存放資料庫的連接字串-->
		<add name="OracleDbConnection"
			 connectionString="Data Source=172.16.63.106:1521/xbos2;User Id=xbosprod;Password=login123;"
